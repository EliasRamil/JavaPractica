package modelo;

import java.util.Arrays;

public class ArrayBidimensional {
	private double[][] matrizA;

	public ArrayBidimensional(double[][] matrizA) {
		this.matrizA = matrizA;
	}
	
	
	//Metodos:
	public double[][] sumar(double[][] matrizB) {
		double[][] matrizSumada = new double[matrizA.length][matrizA.length];
		
		
		
		return matrizSumada;
	}

	public double[][] getMatrizA() {
		return matrizA;
	}

	public void setMatrizA(double[][] matrizA) {
		this.matrizA = matrizA;
	}

	@Override
	public String toString() {
		return "ArrayBidimensional [matrizA=" + Arrays.toString(matrizA) + "]";
	}
	
}
