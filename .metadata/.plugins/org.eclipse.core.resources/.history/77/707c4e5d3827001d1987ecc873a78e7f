		Paciente[] vectorPacientes = new Paciente[3];

		// Paciente p1 = new Paciente("José", "Pérez",1.80f,85f);
		// Paciente p2 = new Paciente("Jorge", "Fernández",1.60f,90f);
		// Paciente p3 = new Paciente("Luciana", "Sachez",1.62f, 79f);

		vectorPacientes[0] = new Paciente("José", "Pérez", 1.80f, 85f);
