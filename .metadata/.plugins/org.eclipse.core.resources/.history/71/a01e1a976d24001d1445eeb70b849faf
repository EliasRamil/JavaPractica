package test;

import modelo.ArrayUnidimensional;
import java.util.Random;

public class testArrayUnidimensional {
	
	public static void main(String[] args) {
		final int TAMANIO = 5;
		Random aleatorio = new Random();
		int[] vector = new int[TAMANIO];
		
		for(int i=0; i<TAMANIO; i++) {
			vector[i] = aleatorio.nextInt(101);
		}
		
		final String LINEA = "--------------------------------------------------------------------------------------------------";
		
		ArrayUnidimensional array = new ArrayUnidimensional(vector);
		
		System.out.println("Datos del vector:\n" + array.toString());
		
		System.out.println("\n" + LINEA + "\nTraer numero mayor del vector: " + array.traerElMayor());
		System.out.println("\n" + LINEA + "\nTraer numero menor del vector: " + array.traerElMenor());
		
	}

}
